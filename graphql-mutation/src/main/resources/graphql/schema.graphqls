



type Query{
    findAllBooks:[Book]
}

type Mutation{
    createBook(title:String,pages:Int,author:String): Book
    addBook(bookInput: BookInput!) : Book
    batchCreate(books:[BookInput!]!):[Book]
}

input BookInput{
    title: String
    pages: Int
    author: String
}


type Book{
    id: ID
    title: String
    pages: Int
    author:String
    reviews: [Review]
}

type Review{
    id: ID
    title: String
    comment: String
    book: Book
}